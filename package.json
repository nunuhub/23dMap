{
  "name": "shinegis-client-23d",
  "version": "2.1.0-2",
  "description": "A Component Library for WebGIS.",
  "main": "lib/shinegis-client-23d.common.js",
  "files": [
    "build/earth-webpack-config",
    "lib",
    "types"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "cz": "czg",
    "bootstrap": "npm install",
    "new": "node build/bin/new.js",
    "build:file": "node build/bin/build-entry.js & node build/bin/i18n.js & node build/bin/version.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG-LOCAL.md -s -r 0",
    "copy:assets": "gulp --gulpfile build/gulp-copy-assets.js",
    "obf:src": "gulp --gulpfile build/gulp-obfuscator.js",
    "clean": "rimraf lib && rimraf src_temp && rimraf packages/*/lib && rimraf test/**/coverage",
    "deploy:build": "rimraf examples/shinegis-client-23d && npm run build:file && cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "dev": "npm run bootstrap && npm run build:file && node build/bin/play.js && cross-env NODE_ENV=development webpack server --config build/webpack.demo.js & node build/bin/template.js",
    "dev:play": "npm run build:file && node build/bin/play.js && cross-env NODE_ENV=development PLAY_ENV=true webpack server --config build/webpack.demo.js",
    "dist": "npm run clean && npm run build:file && npm run lint && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:utils && npm run copy:assets && npm run build:theme",
    "i18n": "node build/bin/i18n.js",
    "lint": "eslint src/**/* packages/**/* build/**/* --quiet",
    "indices": "node build/bin/gen-indices.js",
    "pub:source": "npm run bootstrap && sh build/git-release.sh && sh build/release.sh",
    "pub": "npm run bootstrap && sh build/git-release.sh && sh build/release-obf.sh",
    "test": "npm run lint && npm run build:theme && cross-env CI_ENV=/dev/ BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test:watch": "npm run build:theme && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
